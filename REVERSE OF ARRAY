#include<stdio.h>
int main()
{
    int a[10],b[10],i,n;
    printf("enter size of an array\n");
    scanf("%d",&n);
    printf("enter elements in an array\n");
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    for (i=0;i<n;i++)
    {
        b[i]=a[n-i-1];
    }
        printf("reverse of an array\n");
         for(i=0;i<n;i++){
             printf("%d\n",b[i]);}
             return 0;
    
}
